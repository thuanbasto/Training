import java.util.ArrayList;
import java.util.HashMap;
import java.util.Iterator;
import java.util.List;
import java.util.Map;

public class Count8_3 {
	public static long count8_3(int[] a){
		long result = 0;
		Map<Integer, Integer> map8 = new HashMap<Integer, Integer>();
		Map<Integer, Integer> map3 = new HashMap<Integer, Integer>();
		
		for (int i = 0; i < a.length; i++) {
			int temp = 0;
			if (a[i] % 8 == 0) {
				temp = a[i]/8;
				if (map8.get(temp) == null) 
					map8.put(temp,1);
				else 
					map8.put(temp, map8.get(temp)+1);
			}
			if (a[i] % 3 == 0) {
				temp = a[i]/3;
				if (map3.get(temp) == null) 
					map3.put(temp,1);
				else 
					map3.put(temp, map3.get(temp)+1);
			}
		}
		for (int i : map8.keySet()) {
			if (map3.get(i) != null) {
				result += map3.get(i) + map8.get(i);
			}
		}
		return result;
	}

	public static void main(String[] args) {
		int a[] = {8,3,6,16};
		System.out.println(count8_3(a));
	}

}
